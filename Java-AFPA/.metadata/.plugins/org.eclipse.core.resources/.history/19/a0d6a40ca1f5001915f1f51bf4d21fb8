import java.util.EventObject;

public class FormEvent extends EventObject {
	
	private char lettre;
	
	public FormEvent(Object source) {
		super(source);
	}
	
	public FormEvent(Object source, char lettre) {
		super(source);
		
		this.lettre = lettre;
	}
	
	public char getLettre() {
		return this.lettre;
	}

	public void setLettre(char name) {
		this.lettre = lettre;
	}
}
